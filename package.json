{
  "name": "poc-prisma",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:watch": "jest --watchAll --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:seed": "npm run test:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/app.ts' -e ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "joi": "^17.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.4.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "typescript-transform-paths": "^3.4.6"
  }
}
